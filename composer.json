{
    "name": "athulr/laravel-qr",
    "description": "QR code generator for Laravel",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Athulr\\LaravelQr\\": "src/"
        }
    },
    "require": {
        "bacon/bacon-qr-code": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Athulr\\LaravelQr\\QrCodeServiceProvider"
            ],
            "aliases": {
                "QrCode": "Athulr\\LaravelQr\\Facades\\QrCode"
            }
        }
    }
}